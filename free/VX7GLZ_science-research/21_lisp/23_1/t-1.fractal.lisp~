;;;;;;;;;;;; TINK-1 ;;;;;;;;;;;;
(print ";;;;;;;;;;;; TINK-1 ;;;;;;;;;;;;")

(defstruct (bar
	     (:constructor create-bar (a b))
	     (:constructor create-bar-x (x &aux (a (* x 10))
	     
					   (b (* x 20))))
	     (:constructor make-bar)
	     )
  a b
  )

(print "(create-bar 1 2) =>")
(print (create-bar 1 2))

(print "(create-bar-x 5) =>")
(print (create-bar-x 5))

(print "(make-bar) =>")
(print (make-bar))

;;;;;;;;;;;; TINK-1.1 ;;;;;;;;;;;;
(print ";;;;;;;;;;;; TINK-1.1 ;;;;;;;;;;;;")

(let* ((a 10) (b (* a 10)))
  (format t "a = ~d, b = ~d~%" a b


;;;;;; eof
(print "done")




